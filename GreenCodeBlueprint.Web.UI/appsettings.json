{
  "$schema": "appsettings-schema.json",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Warning"
      }
    }
  },
  "Umbraco": {
    "CMS": {
      "Global": {
        "Id": "c0723b24-abb4-4e4c-80a0-13381632bd34",
        "SanitizeTinyMce": true
      },
      "Content": {
        "AllowEditInvariantFromNonDefault": true,
        "ContentVersionCleanupPolicy": {
          "EnableCleanup": true
        },
        "LoginBackgroundImage": ""
      },
      "ModelsBuilder": {
        "ModelsMode": "SourceCodeAuto",
        "ModelsNamespace": "GreenCodeBlueprint.Web.Models.ModelsBuilder",
        "FlagOutOfDateModels": true,
        "ModelsDirectory": "~/../GreenCodeBlueprint.Web/Models/ModelsBuilder",
        "AcceptUnsafeModelsDirectory": true
      },
      "Security": {
        "AllowConcurrentLogins": false
      },
      "Unattended": {
        "InstallUnattended": true,
        "UpgradeUnattended": true,
        "UnattendedUserName": "Admin",
        "UnattendedUserEmail": "admin@example.com",
        "UnattendedUserPassword": "1234567890"
      }
    }
  },
  "SeoToolkit": {
    "Redirects": {
      "DisabledModules": ["All"]
    }
  }
}
